@article{iwama_abundance_2007,
	title = {Abundance of {microRNA} target motifs in the 3'-{UTRs} of 20 527 human genes},
	volume = {581},
	issn = {0014-5793},
	url = {http://www.sciencedirect.com/science/article/pii/S0014579307003444},
	doi = {10.1016/j.febslet.2007.03.066},
	abstract = {A mechanism, selective avoidance, proposes that {microRNA} ({miRNA)} target sites are selectively depleted in the 3â€²-{UTRs} of genes expressed at the same time and place as a {miRNA.} If this mechanism is ubiquitous, the target motif occurrences in the 3â€²-{UTRs} would be decreased. To test this hypothesis, we examined {miRNA} target motif occurrences in the 3â€²- and 5â€²-{UTRs} of 20\&\#xa0;527 human protein-coding genes. The results revealed that {miRNA} target motifs appeared more frequently than non-target motifs and were enriched in the 3â€²-{UTRs.} This enrichment was relatively reduced in a set of 2525 genes coexpressed with {miR-124a} in the prefrontal cortex, but still remained at a high level, suggesting that {miRNA} target motifs are fostered by some other factors that surpass the influence of selective avoidance.},
	number = {9},
	urldate = {2011-10-20},
	journal = {{FEBS} Letters},
	author = {Iwama, Hisakazu and Masaki, Tsutomu and Kuriyama, Shigeki},
	month = may,
	year = {2007},
	keywords = {Evolution of regulatory network, {MicroRNA}, Non-coding, Selective avoidance},
	pages = {1805--1810},
