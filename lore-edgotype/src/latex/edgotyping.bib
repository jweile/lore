	pages = {258 --261},
	annote = {Lee et al-{\textgreater} scoring},
	file = {258.full.pdf:/Users/jweile/Library/Application Support/Zotero/Profiles/3eksr886.default/zotero/storage/NUQ4FV2T/258.full.pdf:application/pdf;Snapshot:/Users/jweile/Library/Application Support/Zotero/Profiles/3eksr886.default/zotero/storage/NVGK4X93/258.html:text/html}
