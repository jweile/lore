	author = {Drăghici, Sorin and Sellamuthu, Sivakumar and Khatri, Purvesh},
	month = dec,
	year = {2006},
	pages = {2934--2939},
	file = {Full Text PDF:/Users/jweile/Library/Application Support/Zotero/Profiles/3eksr886.default/zotero/storage/GF5AVAK9/Drăghici et al. - 2006 - Babel's tower revisited a universal resource for .pdf:application/pdf;Snapshot:/Users/jweile/Library/Application Support/Zotero/Profiles/3eksr886.default/zotero/storage/K5ISRDIX/Drăghici et al. - 2006 - Babel's tower revisited a universal resource for .html:text/html}
