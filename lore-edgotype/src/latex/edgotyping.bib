	file = {PLoS Full Text PDF:/Users/jweile/Library/Application Support/Zotero/Profiles/3eksr886.default/zotero/storage/K3PNR64T/Michaut and Bader - 2012 - Multiple Genetic Interaction Experiments Provide C.pdf:application/pdf}
