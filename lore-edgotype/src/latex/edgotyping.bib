@article{gunning_differential_1990,
	title = {Differential control of tropomyosin {mRNA} levels during myogenesis suggests the existence of an isoform competition-autoregulatory compensation control mechanism},
	volume = {138},
	issn = {0012-1606},
	url = {http://www.sciencedirect.com/science/article/pii/001216069090210A},
	doi = {10.1016/0012-1606(90)90210-A},
	abstract = {We have isolated tropomyosin {cDNAs} from human skeletal muscle and nonmuscle {cDNA} libraries and constructed gene-specific {DNA} probes for each of the four functional tropomyosin genes. These {DNA} probes were used to define the regulation of the corresponding {mRNAs} during the process of myogenesis. Tropomyosin regulation was compared with that of β- and γ-actin. No two striated muscle-specific tropomyosin {mRNAs} are coordinately accumulated during myogenesis nor in adult striated muscles. Similarly, no two nonmuscle tropomyosins are coordinately repressed during myogenesis. However, {mRNAs} encoding the 248 amino acid nonmuscle tropomyosins and β- and γ-actin are more persistent in adult skeletal muscle than those encoding the 284 amino acid nonmuscle tropomyosins. In particular, the nonmuscle tropomyosin Tm4 is expressed at similar levels in adult rat nonmuscle and striated muscle tissues. We conclude that each tropomyosin {mRNA} has its own unique determinants of accumulation and that the 248 amino acid nonmuscle tropomyosins may have a role in the architecture of the adult myofiber. The variable regulation of nonmuscle isoforms during myogenesis suggests that the different isoforms compete for inclusion into cellular structures and that compensating autoregulation of {mRNA} levels bring gene expression into alignment with the competitiveness of each individual gene product. Such an isoform competition-autoregulatory compensation mechanism would readily explain the unique regulation of each gene.},
	number = {2},
	urldate = {2012-10-22},
	journal = {Developmental Biology},
	author = {Gunning, P. and Gordon, M. and Wade, R. and Gahlmann, R. and Lin, C.-S. and Hardeman, E.},
	month = apr,
	year = {1990},
	pages = {443--453},
	file = {ScienceDirect Full Text PDF:/Users/jweile/Library/Application Support/Zotero/Profiles/3eksr886.default/zotero/storage/S7GN62P7/Gunning et al. - 1990 - Differential control of tropomyosin mRNA levels du.pdf:application/pdf;ScienceDirect Snapshot:/Users/jweile/Library/Application Support/Zotero/Profiles/3eksr886.default/zotero/storage/ZD4MEW9G/Gunning et al. - 1990 - Differential control of tropomyosin mRNA levels du.html:text/html}
},

