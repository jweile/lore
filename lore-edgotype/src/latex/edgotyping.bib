	note = {{PMID:} 16965176},
	keywords = {Abstracting and Indexing as Topic, Algorithms, Artificial Intelligence, Automation, Biomedical Research, Cocaine, Computational Biology, Computer Simulation, Sensitivity and Specificity},
	pages = {e118}
},

